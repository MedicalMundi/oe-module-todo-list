{
    "name": "medicalmundi/oe-module-todo-list",
    "type": "openemr-module",
    "description": "Todo list OpenEMR module.",
    "keywords": [
        "open-emr",
        "openemr-module",
        "todo-list"
    ],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Teclai Zerai",
            "email": "teclaizerai@gmail.com"
        },
        {
            "name": "Francesca Bonadonna",
            "email": "francescabonadonna@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laminas/laminas-httphandlerrunner": "^1.1",
        "league/route": "^4.5",
        "nyholm/psr7": "^1.2",
        "nyholm/psr7-server": "^0.4.1",
        "openemr/oe-module-installer-plugin": "^0.1",
        "psr/container": "^1.0",
        "symfony/config": "^3.4 || ^4.4",
        "symfony/dependency-injection": "^3.4 || ^4.4",
        "symfony/event-dispatcher": "^3.4 || ^4.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.9",
        "phpunit/phpunit": "~8.4",
        "symfony/var-dumper": "^3.4",
        "vimeo/psalm": "^3.11"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "OpenEMR\\Modules\\MedicalMundiTodoList\\": "src/",
            "MedicalMundi\\TodoList\\": "todo-list/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenEMR\\Modules\\MedicalMundiTodoList\\Tests\\": "tests/",
            "MedicalMundi\\TodoList\\Tests\\": "todo-list/tests/"
        }
    },
    "minimum-stability": "stable"
}
