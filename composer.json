{
    "name": "medicalmundi/oe-module-todo-list",
    "type": "openemr-module",
    "description": "Todo list OpenEMR module.",
    "keywords": [
        "open-emr",
        "openemr-module",
        "todo-list"
    ],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Teclai Zerai",
            "email": "teclaizerai@gmail.com"
        },
        {
            "name": "Francesca Bonadonna",
            "email": "francescabonadonna@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "openemr/oe-module-installer-plugin": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "MedicalMundi\\TodoList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MedicalMundi\\TodoList\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
